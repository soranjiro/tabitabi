name = "tabitabi"
compatibility_date = "2024-09-17"
compatibility_flags = ["nodejs_compat"]
main = "apps/web/.svelte-kit/cloudflare/_worker.js"

[assets]
directory = "apps/web/.svelte-kit/cloudflare"
binding = "ASSETS"

[observability]
enabled = true

[[d1_databases]]
binding = "DB"
database_name = "tabitabi"
database_id = "PLACEHOLDER_DATABASE_ID"

[env.production]
VITE_API_URL = "https://tabitabi-api.soranjiro.workers.dev/api/v1"
